import math
x = float(input("enter the x-coordinates of the circle centers:"))
y = float(input("enter the y-coordinates of the circle centers:"))
radius = float(input("enter the radius of the circle:"))
px = float(input("enter the x- coordinate of the point:"))
py = float(input("enter the y- coordinate of the point:"))
distance = math.sqrt(math.pow(px-py, 2) + math.pow(py- y, 2))
if distance < radius:
    print("The point lies inside the circle")
elif(distance==radius):
    print("The point lies on the circle")
else:
    print("The point lies outside the circle")