def separate_student_details(students):
    roll_nos = [student[0] for student in students]
    names = [student[1] for student in students]
    ages = [student[2] for student in students]
    return roll_nos, names, ages

# Example list of students (roll no, name, age)
students = [
    (273, "Santosh", 20),
    (271, "Pradip", 21),
    (309, "Dharam", 20)
]

roll_nos, names, ages = separate_student_details(students)

print("Roll Numbers:", roll_nos)
print("Names:", names)
print("Ages:", ages)
