def print_24_hours():
    for hour in range(24):
        if hour == 0:
            print("12:00 Am - Midnight")
        elif hour == 12:
            print("12:00 Pm - Noon")
        elif hour < 12:
            print(f"{hour}:00 - Am")
        else:
            print(f"{hour - 12}:00 - Pm")
print_24_hours()