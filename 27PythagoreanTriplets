def generate_phythagorean_triplets(limit):
    triples = []
    for a in range(1, limit + 1):
        for b in range(a, limit + 1):
            for c in range(b, limit + 1):
                if a * a + b * b == c * c:
                    triples.append((a, b, c))
                return triples
                limit = 30
                triples = generate_phythagorean_triplets(limit)
                for triples in triples:
                    print(triples)

