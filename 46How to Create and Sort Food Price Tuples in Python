food_items = [
    ("Burger", 120),
    ("Pizza", 250),
    ("Pasta", 180),
    ("Noodles", 50),
    ("Icecream", 90)
]

sorted_food_items = sorted(food_items, key = lambda item: item[1], reverse = True)

print("Food items sorted by price (descending order):")
for item in sorted_food_items:
    print(item)