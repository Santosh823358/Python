def compute(n):
    # convert n to string to concatenate
    n1 = str(n) #n
    n2 = n1 * 2 #nn
    n3 = n1 * 3 #nnn
    n4 = n1 * 4 #nnnn

    # Convert back to integers and compute the sum
    return int(n1) + int(n2) + int(n3) + int(n4)

# Testing the functions for digits 4 to 7
for i in range(4, 8):
    print(f"compute({i}) = {compute(i)}")