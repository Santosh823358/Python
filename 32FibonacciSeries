N = int(input("enter the number of fibonacci series to be printed: "))
a, b = 0, 1
if N < 0:
    print("please enter a positive integer..")
elif N == 1:
    print("Fibonacci series:", a)
else:
    print("Fibonacci series:")
    for i in range(N):
        print(a, end=" ")
        a, b = b, a + b
